#################################################################
#    Object Properties
#################################################################

###  https://linked.data.gov.au/def/smurf#hasGeometryProvenance
smurf:hasGeometryProvenance 
                       rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf <http://www.w3.org/ns/prov#wasDerivedFrom> ;
                       rdfs:domain <http://www.opengis.net/ont/geosparql#Geometry> ;
                       rdfs:range <http://purl.org/linked-data/cube#DataSet> ;
                       rdfs:comment "hasGeometryProvenance indicates the provenance (i.e., source, quality, and other metadata) of a GeoSPARQL Geometry in terms of a DCAT Dataset instance."@en ;
                       rdfs:label "hasGeometryProvenance"@en ;
                       skos:definition "hasGeometryProvenance indicates the provenance (i.e., source, quality, and other metadata) of a GeoSPARQL Geometry in terms of a DCAT Dataset instance."@en ;
                       skos:example "Geometry x is from (hasGeometryProvenance) Dataset y"@en .


#################################################################
#   Data properties
#################################################################

#  Identifiers
#########################################
#https://linked.data.gov.au/def/smurf#PFI introduce smurf:PFI as sub data property of schema:identifier https://schema.org/identifier
smurf:PFI 
          rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf <https://schema.org/identifier> ; 
          rdfs:domain <http://www.opengis.net/ont/geosparql#Geometry> ;
          rdfs:range xsd:integer ;
          rdfs:comment "Indicates the VicMap PFI (persistent feature identifier) for this Geometry"@en ;
          rdfs:label "PFI (Persistent Feature Identifier)"@en ;
          skos:definition "Indicates the VicMap PFI (Persistent Feature Identifier) for this Geometry"@en .


###  https://linked.data.gov.au/def/smurf#UFI introduce smurf:UFI as sub data property of schema:identifier https://schema.org/identifier
smurf:UFI 
          rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf <https://schema.org/identifier> ;
          rdfs:domain <http://www.opengis.net/ont/geosparql#Geometry> ;
          rdfs:range xsd:integer ;
          rdfs:comment "Indicates the VicMap UFI (unique feature identifier) for this Geometry"@en ;
          rdfs:label "UFI (Unique Feature Identifier)"@en ;
          skos:definition "Indicates the VicMap UFI (Unique Feature Identifier) for this Geometry"@en .

### https://linked.data.gov.au/def/smurf#UFI introduce smurf:waterbodyID as sub data property of schema:identifier https://schema.org/identifier
smurf:waterbodyID 
             rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf <https://schema.org/identifier> ;
             rdfs:domain <http://www.opengis.net/ont/geosparql#Feature> ;
             rdfs:range xsd:integer ;
             rdfs:comment "Vicmap waterbody identifier"@en ;
             rdfs:label "Waterbody ID"@en ;
             skos:definition "Vicmap waterbody identifier"@en .

# Additional Feature Property
###################################################################

###  https://linked.data.gov.au/def/smurf#isCrown introduce smurf:isCrown as sub data property of schema:additionalProperty https://schema.org/additionalProperty
smurf:isCrown 
         rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf <https://schema.org/additionalProperty> ;
         rdfs:domain <http://www.opengis.net/ont/geosparql#Geometry> ;
         rdfs:range xsd:boolean ;
         rdfs:comment "Flag to indicate if this data item is a Crown land (true) or not (false)"@en ;
         rdfs:label "Is Crown"@en ;
         skos:definition "Flag to indicate if this data item is a Crown land (true) or not (false)"@en .

###  https://linked.data.gov.au/def/smurf#isAuthoritativeData introduce smurf:isAuthoritativeData as sub data property of schema:additionalProperty https://schema.org/additionalProperty
smurf:isAuthoritativeData 
                     rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf <https://schema.org/additionalProperty> ;
                     rdfs:domain <http://www.w3.org/ns/dqv#QualityMeasurement> ;
                     rdfs:range xsd:boolean ;
                     rdfs:comment "Flag to indicate if this data item is an authoritative VicMap version (true) or not (false)"@en ;
                     rdfs:label "Is authoritative data version"@en ;
                     skos:definition "Flag to indicate if this data item is an authoritative VicMap version (true) or not (false)"@en .

###  https://linked.data.gov.au/def/smurf#varietyOf introduce smurf:varietyOf sub data property of schema:additionalType  https://schema.org/additionalType
smurf:varietyOf 
           rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf <https://schema.org/additionalType> ;
           rdfs:domain <http://www.opengis.net/ont/geosparql#Geometry> ;
           rdfs:range xsd:string ;
           rdfs:comment "Water body type attribute, indicating the type of hydrology feature"@en ;
           rdfs:label "Variety of"@en ;
           skos:definition "Water body type attribute, indicating the type of hydrology feature"@en .

